        <Method>
            <MethodName>check</MethodName>
            <MethodComment>/** 
 * Checks the prescription bean for interactions, allergies, and legal  values.
 * @param pres The prescription bean.
 * @throws iTrustException
 */
</MethodComment>
            <ReturnType>void</ReturnType>
            <ParameterList>
                <ParameterType>PrescriptionBean pres</ParameterType>
            </ParameterList>
            <InnerVarList>
                <InnerVar>List&lt;String&gt; [warnings=checkInteraction(pres)]</InnerVar>
            </InnerVarList>
            <InnerMethodInvokeList>
                <InnerMethodInvoke>warnings;addAll;[checkAllergy(pres)]</InnerMethodInvoke>
            </InnerMethodInvokeList>
            <ThrowExceptionList>
                <ExceptionType>iTrustException</ExceptionType>
            </ThrowExceptionList>
        </Method>
        <Method>
