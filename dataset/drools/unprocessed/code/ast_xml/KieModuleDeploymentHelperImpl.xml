                    <ParamName>dependencies</ParamName>
                    <ParamType>String</ParamType>
                </Parameter>
            </ParameterList>
            <InnerVarList>
                <InnerVar>ReleaseId[] [releaseIds={}]</InnerVar>
                <InnerVar>KieFileSystem [kfs=createKieFileSystemWithKProject(kbaseName,ksessionName)]</InnerVar>
                <InnerVar>List&lt;KJarResource&gt; [resourceFiles=loadResources(resourceFilePaths)]</InnerVar>
                <InnerVar>KieBuilder [kieBuilder=config.getKieServicesInstance().newKieBuilder(kfs)]</InnerVar>
