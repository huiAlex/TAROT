<Class>
    <Id>1383</Id>
    <Package>org.apache.derby.impl.tools.dblook</Package>
    <ClassName>DB_Check</ClassName>
    <SuperClass></SuperClass>
    <SuperInterfaceList>
        <SuperInterface></SuperInterface>
    </SuperInterfaceList>
    <ClassComment></ClassComment>
    <FieldList/>
    <MethodList>
        <Method>
            <MethodName>doChecks</MethodName>
            <MethodComment></MethodComment>
            <ReturnType>void</ReturnType>
            <ParameterList>
                <Parameter>
                    <ParamName>conn</ParamName>
                    <ParamType>Connection</ParamType>
                </Parameter>
            </ParameterList>
            <InnerVarList>
                <InnerVar>Statement [stmt=conn.createStatement()]</InnerVar>
                <InnerVar>ResultSet [rs=stmt.executeQuery("SELECT CS.CONSTRAINTNAME, " + "CS.TABLEID, CS.SCHEMAID, CS.STATE, CK.CHECKDEFINITION FROM SYS.SYSCONSTRAINTS CS, " + "SYS.SYSCHECKS CK WHERE CS.CONSTRAINTID = "+ "CK.CONSTRAINTID AND CS.STATE != 'D' ORDER BY CS.TABLEID")]</InnerVar>
                <InnerVar>boolean [firstTime=true]</InnerVar>
            </InnerVarList>
            <InnerMethodInvokeList>
                <InnerMethodInvoke>stmt;close;[]</InnerMethodInvoke>
                <InnerMethodInvoke>rs;close;[]</InnerMethodInvoke>
            </InnerMethodInvokeList>
            <ThrowExceptionList>
                <ExceptionType>SQLException</ExceptionType>
            </ThrowExceptionList>
        </Method>
        <Method>
            <MethodName>createCheckString</MethodName>
            <MethodComment></MethodComment>
            <ReturnType>StringBuffer</ReturnType>
            <ParameterList>
                <Parameter>
                    <ParamName>tableName</ParamName>
                    <ParamType>String</ParamType>
                </Parameter>
                <Parameter>
                    <ParamName>aCheck</ParamName>
                    <ParamType>ResultSet</ParamType>
                </Parameter>
            </ParameterList>
            <InnerVarList>
                <InnerVar>StringBuffer [sb=new StringBuffer("ALTER TABLE ")]</InnerVar>
                <InnerVar>String [constraintName=dblook.addQuotes(dblook.expandDoubleQuotes(aCheck.getString(1)))]</InnerVar>
            </InnerVarList>
            <InnerMethodInvokeList>
                <InnerMethodInvoke>sb;append;[tableName]</InnerMethodInvoke>
                <InnerMethodInvoke>sb;append;[" ADD"]</InnerMethodInvoke>
                <InnerMethodInvoke>sb;append;[" CONSTRAINT "]</InnerMethodInvoke>
                <InnerMethodInvoke>sb;append;[constraintName]</InnerMethodInvoke>
                <InnerMethodInvoke>sb;append;[" CHECK "]</InnerMethodInvoke>
                <InnerMethodInvoke>sb;append;[dblook.removeNewlines(aCheck.getString(5))]</InnerMethodInvoke>
            </InnerMethodInvokeList>
            <ThrowExceptionList>
                <ExceptionType>SQLException</ExceptionType>
            </ThrowExceptionList>
        </Method>
    </MethodList>
</Class>