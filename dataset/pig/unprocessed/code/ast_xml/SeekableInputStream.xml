    <FieldList/>
    <MethodList>
        <Method>
            <MethodName>seek</MethodName>
            <MethodComment>/** 
 * Seeks to a given offset as specified by whence flags. If whence is SEEK_SET, offset is added to beginning of stream If whence is SEEK_CUR, offset is added to current position inside stream If whence is SEEK_END, offset is added to end of file position
 * @param offset
 * @param whence
 * @throws IOException
 */
</MethodComment>
            <ReturnType>void</ReturnType>
            <ParameterList>
                <Parameter>
                    <ParamName>offset</ParamName>
                    <ParamType>long</ParamType>
                </Parameter>
                <Parameter>
                    <ParamName>whence</ParamName>
                    <ParamType>FLAGS</ParamType>
                </Parameter>
            </ParameterList>
            <ThrowExceptionList>
                <ExceptionType>IOException</ExceptionType>
            </ThrowExceptionList>
        </Method>
        <Method>
            <MethodName>tell</MethodName>
            <MethodComment>/** 
 * Returns current offset
 * @return offset
 * @throws IOException
 */
</MethodComment>
            <ReturnType>long</ReturnType>
            <ParameterList/>
            <ThrowExceptionList>
                <ExceptionType>IOException</ExceptionType>
            </ThrowExceptionList>
        </Method>
    </MethodList>
