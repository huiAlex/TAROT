            </ParameterList>
            <InnerVarList>
                <InnerVar>Map&lt;Integer,Set&lt;K&gt;&gt; [contextMap=new HashMap&lt;&gt;()]</InnerVar>
            </InnerVarList>
            <InnerMethodInvokeList>
                <InnerMethodInvoke>ctx.getLookedUpEntries();forEach;[(k,v) -&gt; {
  Integer segment=hash.getSegment(k);
  Set&lt;K&gt; innerSet=contextMap.get(segment);
  if (innerSet == null) {
    innerSet=new HashSet&lt;K&gt;();
    contextMap.put(segment,innerSet);
  }
  innerSet.add((K)k);
}
]</InnerMethodInvoke>
