            <InnerVarList>
                <InnerVar>PathAddress [address=PathAddress.pathAddress(operation.require(OP_ADDR))]</InnerVar>
                <InnerVar>String [cacheContainerName=address.getElement(address.size() - 2).getValue()]</InnerVar>
                <InnerVar>String [cacheName=address.getLastElement().getValue()]</InnerVar>
                <InnerVar>String [attrName=operation.require(NAME).asString()]</InnerVar>
                <InnerVar>ServiceController&lt;?&gt; [controller=context.getServiceRegistry(false).getService(CacheServiceName.CACHE.getServiceName(cacheContainerName,cacheName))]</InnerVar>
                <InnerVar>Cache&lt;?,?&gt; [cache=(Cache&lt;?,?&gt;)controller.getValue()]</InnerVar>
                <InnerVar>CacheMetrics [metric=CacheMetrics.getStat(attrName)]</InnerVar>
                <InnerVar>ModelNode [result=new ModelNode()]</InnerVar>
            </InnerVarList>
            <InnerMethodInvokeList>
                <InnerMethodInvoke>context;stepCompleted;[]</InnerMethodInvoke>
            </InnerMethodInvokeList>
