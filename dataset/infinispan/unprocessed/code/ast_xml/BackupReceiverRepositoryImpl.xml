        <Method>
            <MethodName>getBackupReceiver</MethodName>
            <MethodComment>/** 
 * Returns the local cache defined as backup for the provided remote (site, cache) combo, or throws an exception if no such site is defined. &lt;p/&gt; Also starts the cache if not already started; that is because the cache is needed for update after this method is invoked.
 */
</MethodComment>
            <ReturnType>BackupReceiver</ReturnType>
            <ParameterList>
                <Parameter>
                    <ParamName>remoteSite</ParamName>
                    <ParamType>String</ParamType>
                </Parameter>
                <Parameter>
                    <ParamName>remoteCache</ParamName>
                    <ParamType>String</ParamType>
                </Parameter>
            </ParameterList>
            <InnerVarList>
                <InnerVar>SiteCachePair [toLookFor=new SiteCachePair(remoteCache,remoteSite)]</InnerVar>
                <InnerVar>BackupReceiver [backupManager=backupReceivers.get(toLookFor)]</InnerVar>
                <InnerVar>Configuration [dcc=cacheManager.getDefaultCacheConfiguration()]</InnerVar>
                <InnerVar>Set&lt;String&gt; [cacheNames=cacheManager.getCacheNames()]</InnerVar>
                <InnerVar>Cache&lt;Object,Object&gt; [cache=cacheManager.getCache(remoteCache)]</InnerVar>
            </InnerVarList>
            <InnerMethodInvokeList>
                <InnerMethodInvoke>log;debugf;["Did not find any backup explicitly configured backup cache for remote cache/site: %s/%s. Using %s", remoteSite, remoteCache, remoteCache]</InnerMethodInvoke>
                <InnerMethodInvoke>backupReceivers;putIfAbsent;[toLookFor, createBackupReceiver(cache)]</InnerMethodInvoke>
                <InnerMethodInvoke>toLookFor;setLocalCacheName;[cache.getName()]</InnerMethodInvoke>
            </InnerMethodInvokeList>
            <ThrowExceptionList>
                <ExceptionType></ExceptionType>
            </ThrowExceptionList>
        </Method>
