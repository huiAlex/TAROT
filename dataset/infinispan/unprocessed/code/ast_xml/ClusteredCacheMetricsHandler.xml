                <InnerVar>String [attrName=operation.require(NAME).asString()]</InnerVar>
                <InnerVar>ServiceController&lt;?&gt; [controller=context.getServiceRegistry(false).getService(CacheServiceName.CACHE.getServiceName(cacheContainerName,cacheName))]</InnerVar>
                <InnerVar>Cache&lt;?,?&gt; [cache=(Cache&lt;?,?&gt;)controller.getValue()]</InnerVar>
                <InnerVar>ClusteredCacheMetrics [metric=ClusteredCacheMetrics.getStat(attrName)]</InnerVar>
                <InnerVar>ModelNode [result=new ModelNode()]</InnerVar>
            </InnerVarList>
